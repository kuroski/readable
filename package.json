{
  "name": "readable",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "create-react-class": "^15.6.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.2.1",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "i18n-react": "^0.6.2",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^4.3.0",
    "normalize.css": "^7.0.0",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.0.0",
    "react-throttle": "^0.3.0",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.0.0-beta.3",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^1.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom --updateSnapshot",
    "eject": "react-scripts eject",
    "coverage": "yarn test --coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-links": "^3.2.14",
    "@storybook/addon-storyshots": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "cross-env": "^5.1.1",
    "expect": "^21.2.1",
    "redux-devtools-extension": "^2.13.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/common/**/*.js"
    ]
  }
}
